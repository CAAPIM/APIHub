{
    "name": "layer7-apihub-mock",
    "version": "1.0.0",
    "private": true,
    "bin": "bin/generateData.js",
    "main": "lib/index.js",
    "module": "esm/index.js",
    "sideEffects": false,
    "scripts": {
        "build": "yarn run build-cjs && yarn run build-esm",
        "build-cjs": "rimraf ./lib && tsc",
        "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
        "dev": "tsc --watch --outDir esm --module es2015",
        "test": "kcd-scripts test"
    },
    "dependencies": {
        "@stoplight/prism-http": "3.3.5",
        "date-fns": "2.14.0",
        "faker": "Marak/faker.js",
        "kcd-scripts": "6.3.0",
        "lodash": "4.17.15",
        "minimist": "1.2.5",
        "minimongo": "6.5.1",
        "miragejs": "0.1.40",
        "yamljs": "0.3.0"
    },
    "devDependencies": {
        "rimraf": "3.0.2",
        "typescript": "3.7.5"
    },
    "files": [
        "*.md",
        "lib",
        "esm",
        "src"
    ]
}
