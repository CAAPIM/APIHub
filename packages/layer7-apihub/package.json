{
    "name": "layer7-apihub",
    "version": "1.0.0",
    "main": "lib/index.js",
    "module": "esm/index.js",
    "sideEffects": false,
    "scripts": {
        "build": "yarn run build-cjs && yarn run build-esm",
        "build-cjs": "rimraf ./lib && tsc",
        "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
        "dev": "tsc --incremental --watch --outDir esm --module es2015",
        "test": "kcd-scripts test --env=jest-environment-jsdom-sixteen",
        "test:coverage": "kcd-scripts test --env=jest-environment-jsdom-sixteen --coverage"
    },
    "dependencies": {
        "@blackbox-vision/ra-language-spanish": "~3.5.0",
        "@material-ui/core": "~4.9.3",
        "@material-ui/lab": "~4.0.0-alpha.45",
        "@rehooks/local-storage": "~2.4.0",
        "final-form-calculate": "~1.3.2",
        "focus-trap-react": "~6.0.0",
        "jsencrypt": "~3.0.0-rc.1",
        "ra-i18n-polyglot": "~3.6.2",
        "ra-language-english": "~3.6.2",
        "ra-language-french": "~3.6.2",
        "ra-language-italian": "~3.0.0",
        "react-admin": "~3.10.3",
        "react-dnd": "~10.0.2",
        "react-dnd-html5-backend": "~10.0.2",
        "react-markdown-editor-lite": "~1.0.2",
        "trim": "0.0.3",
        "remark-parse": "9.0.0",
        "remark-react": "~7.0.1",
        "remove-markdown": "~0.3.0",
        "slugify": "~1.4.4",
        "swagger-ui-react": "~3.27.0",
        "unified": "~8.4.2"
    },
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "~5.11.4",
        "@testing-library/react": "~11.0.0",
        "@testing-library/user-event": "~12.1.6",
        "fetch-mock": "~9.10.3",
        "identity-obj-proxy": "~3.0.0",
        "jest-environment-jsdom-sixteen": "~1.0.3",
        "jest-localstorage-mock": "~2.4.2",
        "kcd-scripts": "6.3.0",
        "layer7-apihub-mock": "~1.0.0",
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0",
        "react-scripts": "~4.0.1",
        "rimraf": "~3.0.2",
        "typescript": "~4.1.2",
        "recompose":"~0.30.0"
    },
    "files": [
        "*.md",
        "lib",
        "esm",
        "src"
    ]
